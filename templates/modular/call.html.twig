<section id="call-to-action">
  <div class="row">
    <div class="col-md-2 header-col">
      <h1><span>{{ page.header.title }}</span></h1>
    </div>
    <div class="col-md-7">
      {{ page.content }}
    </div>
    <div class="col-md-3 action">
      {% for button in page.header.buttons %}
      <a href="{{ button.url }}" class="button">{{ button.text }}</a>
      {% endfor %}
    </div>
  </div>
</section>
